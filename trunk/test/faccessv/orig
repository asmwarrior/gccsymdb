PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE FunctionPattern (
	callerID integer references Definition (id),
	name text,
	flag integer
);
INSERT INTO "FunctionPattern" VALUES(16,'abc.arr[][].y',4);
