PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE chFile (
	id integer primary key autoincrement,
	name text,
	mtime bigint,
	sysHeader boolean
);
INSERT INTO "chFile" VALUES(1,'test/cpptoken/a.c',1335513056,'false');
CREATE INDEX FileName on chFile (name);
COMMIT;
PRAGMA foreign_keys=OFF;
