PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE chFile (
	id integer primary key autoincrement,
	name text,
	mtime bigint,
	sysHeader boolean
);
INSERT INTO "chFile" VALUES(1,'test/file_dependence/a.c',1325350860,'f');
INSERT INTO "chFile" VALUES(2,'test/file_dependence/a.h',1325350860,'f');
INSERT INTO "chFile" VALUES(3,'test/file_dependence/b.h',1325350860,'f');
INSERT INTO "chFile" VALUES(4,'test/file_dependence/c.h',1325350860,'f');
CREATE INDEX FileName on chFile (name);
CREATE TRIGGER DelFile after delete on chFile
begin
	delete from FileDependence where chID = old.id;
	delete from FileDefinition where fileID = old.id;
	delete from Ifdef where fileID = old.id;
	delete from FunpAlias where fileID = old.id;
end;
COMMIT;
PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE FileDependence (
	chID integer references chFile (id),
	hID integer references chFile (id),
	offset integer
);
INSERT INTO "FileDependence" VALUES(1,2,8);
INSERT INTO "FileDependence" VALUES(2,3,8);
