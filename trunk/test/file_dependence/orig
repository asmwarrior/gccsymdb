PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE Definition (
	id integer primary key autoincrement,
	name text,
	flag integer,
	position integer
);
CREATE INDEX DefName on Definition (name);
COMMIT;
PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE DefinitionRelationship (
	caller bigint,
	callee bigint
);
COMMIT;
PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
