PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE chFile (
	id integer primary key autoincrement,
	name text,
	mtime bigint,
	sysHeader boolean
);
INSERT INTO "chFile" VALUES(1,'test/hash/a.c',1335662753,'false');
