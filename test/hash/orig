PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE Definition (
	id integer primary key autoincrement,
	name text,
	flag integer,
	fileoffset integer
);
INSERT INTO "Definition" VALUES(1,'i',1,25);
CREATE INDEX DefName on Definition (name);
COMMIT;
PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE DefinitionRelationship (
	caller bigint,
	callee bigint
);
COMMIT;
PRAGMA foreign_keys=OFF;
